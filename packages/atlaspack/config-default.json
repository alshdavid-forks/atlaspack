{
  "bundler": "./lib/bundler-default/index.js",
  "transformers": {
    "types:*.{ts,tsx}": ["./lib/transformer-typescript-types/index.js"],
    "bundle-text:*": ["...", "./lib/transformer-inline-string/index.js"],
    "data-url:*": ["...", "./lib/transformer-inline-string/index.js"],
    "worklet:*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "./lib/transformer-worklet/index.js",
      "..."
    ],
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "./lib/transformer-js/index.js",
      "./lib/transformer-react-refresh-wrap/index.js"
    ],
    "*.{json,json5}": ["./lib/transformer-json/index.js"],
    "*.jsonld": ["./lib/transformer-jsonld/index.js"],
    "*.toml": ["./lib/transformer-toml/index.js"],
    "*.webmanifest": ["./lib/transformer-webmanifest/index.js"],
    "webmanifest:*.{json,webmanifest}": [
      "./lib/transformer-webmanifest/index.js"
    ],
    "*.{yaml,yml}": ["./lib/transformer-yaml/index.js"],
    "*.{glsl,vert,frag}": ["./lib/transformer-glsl/index.js"],
    "*.{gql,graphql}": ["./lib/transformer-graphql/index.js"],
    "*.{sass,scss}": ["./lib/transformer-sass/index.js"],
    "*.{css,pcss}": [
      "./lib/transformer-postcss/index.js",
      "./lib/transformer-css/index.js"
    ],
    "*.{htm,html,xhtml}": [
      "./lib/transformer-posthtml/index.js",
      "./lib/transformer-html/index.js"
    ],
    "*.pug": ["./lib/transformer-pug/index.js"],
    "*.mdx": ["./lib/transformer-mdx/index.js"],
    "*.{png,jpg,jpeg,webp,gif,tiff,avif,heic,heif}": [
      "./lib/transformer-image/index.js"
    ],
    "*.svg": ["./lib/transformer-svg/index.js"],
    "*.{xml,rss,atom}": ["./lib/transformer-xml/index.js"],
    "url:*": ["...", "./lib/transformer-raw/index.js"]
  },
  "namers": ["./lib/namer-default/index.js"],
  "runtimes": [
    "./lib/runtime-js/index.js",
    "./lib/runtime-browser-hmr/index.js",
    "./lib/runtime-react-refresh/index.js",
    "./lib/runtime-service-worker/index.js"
  ],
  "optimizers": {
    "data-url:*": ["...", "./lib/optimizer-data-url/index.js"],
    "*.css": ["./lib/optimizer-css/index.js"],
    "*.{html,xhtml}": ["./lib/optimizer-htmlnano/index.js"],
    "*.{js,mjs,cjs}": ["./lib/optimizer-swc/index.js"],
    "*.svg": ["./lib/optimizer-svgo/index.js"],
    "*.{jpg,jpeg,png}": ["./lib/optimizer-image/index.js"]
  },
  "packagers": {
    "*.{html,xhtml}": "./lib/packager-html/index.js",
    "*.css": "./lib/packager-css/index.js",
    "*.{js,mjs,cjs}": "./lib/packager-js/index.js",
    "*.svg": "./lib/packager-svg/index.js",
    "*.{xml,rss,atom}": "./lib/packager-xml/index.js",
    "*.ts": "./lib/packager-ts/index.js",
    "*.wasm": "./lib/packager-wasm/index.js",
    "*.{jsonld,svg,webmanifest}": "./lib/packager-raw-url/index.js",
    "*": "./lib/packager-raw/index.js"
  },
  "compressors": {
    "*": ["./lib/compressor-raw/index.js"]
  },
  "resolvers": ["./lib/resolver-default/index.js"],
  "reporters": ["./lib/reporter-dev-server/index.js"]
}
